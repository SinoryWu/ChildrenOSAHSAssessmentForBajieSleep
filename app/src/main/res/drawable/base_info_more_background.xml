<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="64dp" android:height="30dp" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#ffffffff" />
            <stroke android:color="#DEDDDD" android:width="0.5dp"/>
            <corners android:topLeftRadius="3dp" android:topRightRadius="3dp" android:bottomLeftRadius="3dp" android:bottomRightRadius="3dp" />
        </shape>
    </item>

    <item android:width="64dp" android:height="30dp" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#eeeeee" />
            <stroke android:color="#DEDDDD" android:width="0.5dp"/>
            <corners android:topLeftRadius="3dp" android:topRightRadius="3dp" android:bottomLeftRadius="3dp" android:bottomRightRadius="3dp" />
        </shape>
    </item>
</selector>