<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/item_setting_account_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="机构管理员："
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/guideline51"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view20"
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_setting_account_type" />

    <TextView
        android:id="@+id/item_setting_account_name"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="李二花花滑"
        android:textColor="#262626"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/item_setting_account_type"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_setting_account_mobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="106dp"
        android:text="李二花"
        android:textColor="#262626"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/item_setting_account_type"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="96dp" />
</androidx.constraintlayout.widget.ConstraintLayout>